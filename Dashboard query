Dealer management Dashboard 
SELECT
DDF.DEALER_ID,
  DDF.FILE_ID,
  DDF.STATUS,
  NVL(DFD.BATCH_NO, 'B1') AS BATCH_NO,
  COUNT(DDF.FILE_ID) AS FILE_ASSIGN,
  DFD.CATEGORY,
  DFD.NATURE,

  DDF.APPROVED_ON,
  DDF.APPROVED_BY
FROM
  DGMS_DEALERS_FORMS DDF
JOIN
  DGMS_FILES_DETAILS DFD ON DDF.ID = DFD.ID
WHERE
  DDF.STATUS IN ('TRANSFERRED', 'APPROVED')
  AND  DFD.CATEGORY IS NOT NULL
GROUP BY
  DDF.DEALER_ID,
  DDF.FILE_ID,
  DDF.STATUS,
   NVL(DFD.BATCH_NO, 'B1'),
   DFD.CATEGORY,
   DFD.NATURE,
  DDF.APPROVED_ON,
  DDF.APPROVED_BY;
