javascript:$s('P79_BOOKING_ID','#BOOKING_ID#');javascript:$s('P79_ARREAR','ARREAR NOTICE # 1');




SELECT
    DPD.PLOT_NO AS D , DPD.ID AS R
    
FROM DGMS_PLOT_DETAILS DPD
JOIN DGMS_PLOTS_BOOKING DPB ON DPB.PLOT_ID = DPD.ID 
AND DPB.BOOKING_STATUS IN ('BOOKED','BOOKING IN-PROCESS') AND DPB.ORG_ID = :P_ORG_ID
WHERE DPD.ORG_ID = :P_ORG_ID  
