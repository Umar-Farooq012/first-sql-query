SELECT CLIENT_REG_NO FROM ( SELECT  CLIENT_REG_NO , ROWNUM AS rn FROM DGMS_PLOTS_BOOKING )
       WHERE rn <= 5;

RANK , DANSE_RANK , ROW_NUMBER AND ROWNUM QUERY 

SELECT DEALER_ID ,
ROW_NUMBER () OVER (ORDER BY DEALER_ID) AS ROW_NUMBERS,
RANK () OVER (ORDER BY DEALER_ID) AS RANKS,
DENSE_RANK () OVER (ORDER BY DEALER_ID) AS DENSE_RANKS
FROM DGMS_DEALERS_FORMS
WHERE ROWNUM <=10
